<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="jcorreaT3A.Views.vDos"
             Title="vDos">
    <VerticalStackLayout
        Padding="20"
        Spacing="20">

        <Label x:Name="NombreLabel" />
        <Label x:Name="IdentificacionLabel" />
        <Label x:Name="FechaLabel" />
        <Label x:Name="CorreoLabel" />
        <Label x:Name="SalarioLabel" />
        <Label x:Name="AporteIessLabel" FontAttributes="Bold" />

        <Button Text="Exportar"
                Background="ForestGreen"
                Clicked="Button_Clicked"/>

    </VerticalStackLayout>
</ContentPage>